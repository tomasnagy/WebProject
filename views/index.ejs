<!DOCTYPE html>
<html>
<!--<html manifest="manifest.appcache">-->
<head>
    <title><%= title %></title>
    <link href='http://fonts.googleapis.com/css?family=Tangerine' rel='stylesheet' type='text/css'>
    <link rel="stylesheet" href="/stylesheets/normalize.css"/>
    <link rel='stylesheet' href='/build/stylesheets/style.min.css'/>
    <meta name="viewport" content="width=device-width"/>
    <meta name="viewport" content="initial-scale=1, maximum-scale=1, user-scalable=no"/>
</head>
<body data-ng-app="app">
        <!--background will be absolute, background forces height-->
    <header>
        <h1><%= title%></h1>
        <button id="play-guitar">Play guitar</button>
    </header>
    <div id="data-container" data-ng-controller="HistoryItemController">
        <img src="/build/images/bg4.svg" alt="background"/>

        <inlay data-ng-repeat="i in items"></inlay>

        <section data-ng-repeat="i in items" data-repeat-done="layoutDone()">
            <h2 data-ng-bind="i.title"></h2>
            <h3 data-ng-bind="i.date"></h3>
            <img alt="{{i.title}}"/>
            <p data-ng-bind="i.description"></p>
        </section>
    </div>
    <div id="stage-container">
        <img src="build/images/stage.svg" alt="stage"/>
        <div id="locationloader">
            <img src="build/images/loading-spin.svg" alt="loading spinner"/>
            <h4>Loading band setup...</h4>
            <p>Please make sure you specifically allow or deny use of your location</p>
        </div>
        <figure class="invisible">
            <img src="" alt=""/>
            <div id="chord1"></div>
            <div id="chord2"></div>
            <div id="chord3"></div>
            <div id="chord4"></div>
        </figure>
        <div class="invisible" id="support-guitars">
            <!--<img src="images/guitars/cheap.svg" alt="cheap"/>-->
            <!--<img src="images/guitars/heartBroken.svg" alt="cheap"/>-->
            <!--<img src="images/guitars/practiceSpace.svg" alt="cheap"/>-->
        </div>
        <button id="back-to-top">Back to top</button>
        <p>Click or hover over the chords you want to play, or use Q, S, D, F.</p>
    </div>
    <!--CDN Scripts-->
    <script src="http://cdnjs.cloudflare.com/ajax/libs/gsap/1.14.2/TweenMax.min.js" type="application/javascript"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.2.27/angular.min.js" type="application/javascript"></script>
    <script src="http://code.createjs.com/soundjs-0.5.2.min.js" type="application/javascript"></script>
    <script src="https://cdn.socket.io/socket.io-1.2.1.js" type="text/javascript"></script>
    <script src="http://maps.googleapis.com/maps/api/js?sensor=false" type="text/javascript"></script>

    <!--Own scripts-->
    <!--<script src="javascripts/lib/window.resizeStop.js"></script>-->
    <!--<script src="javascripts/util/pollyfills.js"></script>-->
    <!--<script src="javascripts/util/scrolling.js"></script>-->
    <!--<script src="/javascripts/guitarhistory/animation/HistoryAnimationItem.js"></script>-->
    <!--<script src="/javascripts/guitarhistory/animation/animationsMain.js"></script>-->
    <!--<script src="/javascripts/guitarhistory/app.js" type="text/javascript"></script>-->
    <!--<script src="/javascripts/guitarhistory/model/GuitarHistoryItem.js" type="text/javascript"></script>-->
    <!--<script src="/javascripts/guitarhistory/directive/layoutDoneDirective.js" type="text/javascript"></script>-->
    <!--<script src="/javascripts/guitarhistory/directive/inlayDirective.js" type="text/javascript"></script>-->
    <!--<script src="/javascripts/guitarhistory/controller/HistoryItemController.js" type="text/javascript"></script>-->
    <!--<script src="/javascripts/location/location.js"></script>-->
    <!--<script src="/javascripts/guitarplayer/GuitarPlayerItem.js"></script>-->
    <!--<script src="/javascripts/guitarplayer/guitarPlayerMain.js"></script>-->
    <script src="/build/javascript/app.min.js" type="text/javascript"></script>

</body>
</html>
